//P20. enum

#include<stdio.h>
int main()
{
enum week {MON,TUE,WED,THU,FRI,SAT,SUN};
enum week day ;
printf("%d %d %d %d %d %d %d",MON,TUE,WED,THU,FRI,SAT,SUN);
return 0;
}

//P21. bool

#include<stdio.h>
#include<stdbool.h>
int main()
{
bool n1 = true;
bool n2 = false;
bool n3 = 0;
bool n4 = 123;
printf("%d %d %d %d",n1,n2,n3,n4);
return 0;
}

//P22. variable

#include<stdio.h>
#include<stdbool.h>
int main()
{
int n1 = 123456;

printf("%d",n1);
return 0;
}

//P23. ASCII value of any key

#include<stdio.h>
#include<stdbool.h>
int main()
{
char ch;
printf("enter any key\n");
scanf("%c",&ch);
printf("%d",ch);
return 0;
}

//P24. const int

#include<stdio.h>
int main()
{
const int ch=5;
printf("%d",ch);
return 0;
}

//P25. change the value of the varibale of type constant
"Displays error"
#include<stdio.h>
int main()
{
const int ch=5;
ch=6;
printf("%d",ch);
return 0;
}

//P26. varibale name as any keyword
"Displays error"
#include<stdio.h>
int main()
{
int if =5;
printf("%d",ch);
return 0;
}

//Pe. Print character from ASCII value

#include<stdio.h>
int main()
{
int ch;
printf("enter the ASCII value\n");
scanf("%d",&ch);
printf("%c",ch);
return 0;
}

//Pf. Add two floating point & display result upto 20 decimal points

#include<stdio.h>
int main()
{
    float a=254.14575127404212554256320;
    float b=152.16596532347996649796497;
    float res;
    res=a+b;
    printf("%0.19f",res);
    return 0;
}

//Pg. Uppercase to Lowercase

#include<stdio.h>
int main()
{
    char ch,up;
    printf("enter character:\n");
    scanf("%c",&ch);
    up=toupper(ch);
    printf("%c",up);
    return 0;
}

//Ph. Size of the datatypes

#include <stdio.h>
int main()
{
int n1;
float n2;
char n3;
long n4;
double n5;
unsigned long n6;
printf("%d %d %d %d %d %d %d",sizeof(n1),sizeof(n6),sizeof(n2),sizeof(n3),sizeof(n4),sizeof(n5));
return 0;
}
