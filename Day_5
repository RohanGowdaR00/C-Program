//P54. Chech 2 no. equal or not

  #include<stdio.h>
  int main()
  {
      int a,A;
      printf("enter 2 no:\n");
      scanf("%d %d",&a,&A);
      if(a==A)
        printf("equal");
      else
        printf("not equal");
      return 0;
  }
// P55. Calender
#include<stdio.h>
int main ()
{
    int date, days, mon, total;
    printf("date and month");
    scanf("%d %d", &date, &mon);
    if (mon==1)
    days=31;
    if (mon==2)
    days=59;
    if (mon==3)
    days=90;
    if (mon==4)
    days=120;
    if (mon==5)
    days=151;
    if (mon==6)
    days=181;
    if (mon==7)
    days=212;
    if (mon==8)
    days=243;
    if (mon==9)
    days=273;
    if (mon==10)
    days=304;
    if (mon==11)
    days=334;
    if (mon==12)
    days=365;

    total = days;

    if(mon==1||mon==3||mon==5||mon==7||mon==8||mon==10||mon==12)
        total = total-31+date;
    else if(mon==4||mon==6||mon==9||mon==11)
        total = total-30+date;
    else
        total = total-28+date;

    switch(total%7)
    {
        case 0:
    printf("sat");
    break;
        case 1:
    printf("sun");
    break;

        case 2:
    printf("mon");
    break;

        case 3:
    printf("tue");
    break;

        case 4:
    printf("wed");
    break;

        case 5:
    printf("thur");
    break;
    
        case 6:
    printf("fri");
    break;
    }
}


//P56.Positive ,negative & zero

 #include<stdio.h>
  int main()
  {
  int i;
  printf("enter date:\n");
  scanf("%d",&i);
  if(i<0)
    printf("%d is positive",i);
  else if (i>0)
    printf("%d is negative",i);
  else
    printf("%d is zero",i);
      return 0;
  }

//P57. To check char is vowel or not

#include<stdio.h>
  int main()
  {
  int ch;
  printf("enter char:\n");
  scanf("%c",&ch);
  if(ch=='a'||ch=='e'||ch=='i'||ch=='o'||ch=='u'||ch=='A'||ch=='E'||ch=='I'||ch=='O'||ch=='U')
    printf("%c is vowel",ch);
  else
    printf("%c is not vowel",ch);
  return 0;
  }

//58. Leap year or not

  #include<stdio.h>
  int main()
  {
     int leap_year,next_leap;
     printf("enter year:\n");
     scanf("%d",&leap_year);
     if((leap_year%4==0))
     {
               printf("%d is leap year\n",leap_year);
     }

     else
        printf("%d is not leap year\n",leap_year);
     next_leap=leap_year+4;
         printf("next leap year is %d",next_leap);
      return 0;
  }

//P59. 

  #include<stdio.h>
  int main()
  {
     int mat,sci,eng,kan,his,total,fail=0,pass;
     float avg,percent;
     printf("enter the marks:\n");
     scanf("%d %d %d %d %d",&mat,&sci,&eng,&kan,&his);
     avg=(mat+sci+eng+kan+his)/5;
     total=(mat+sci+eng+kan+his);
     percent=((float)total/500)*100;
     printf("total is %d",total);
     printf("percent is %f",percent);
     if(percent>90)
        printf("Distiction\n");
     else if(percent>60 && percent<90)
        printf("First class\n");
     else if (percent>=35 && percent<60)
        printf("Second class\n");
     else
        printf("Fail\n");
     if(mat<35)
     fail++;
     if(sci<35)
       fail++;
     if(eng<35)
        fail++;
     if(kan<35)
        fail++;
     if(his<35)
        fail++;
     pass=5-fail;
     printf("No. pass subject is %d",pass);
     printf("No. fail subject is %d",fail);
      return 0;
  }

//P60. Largest of 3 numbers

  #include<stdio.h>
  int main()
  {
     int a,b,c;
     printf("enter 3 no.:\n");
     scanf("%d %d %d",&a,&b,&c);
     if(a>b && a>c)
        printf("Greater is %d",a);
     else if (b>a && b>c)
        printf("Greater is %d",b);
     else
        printf("Greater is %d",c);

      return 0;
  }

//P61. Coordinates

  #include<stdio.h>
  int main()
  {
     int x,y;
     printf("enter x&y coord:\n");
     scanf("%d %d",&x,&y);
     if(x>0)
     {
         if(y>0)
             printf("first");
         else
            printf("4th");
     }
     else if(x<0)
     {
         if(y>0)
            printf("2nd");
         else
            printf("3rd");
     }
     else if(x==0 && y==0)
        printf("origin");
     else
        printf("lies on axis");
      return 0;
  }

//P63. temp

  #include<stdio.h>
  int main()
  {
     float temp;
     printf("enter temp:\n");
     scanf("%f",&temp);
     if(temp<0)
        printf("Freezing");
     else if(temp>0 && temp<10)
        printf("very cold");
     else if(temp>10 && temp<20)
        printf("cold");
     else if(temp>20 && temp<30)
        printf("Normal");
        else
            printf("hot");
      return 0;
  }

//P64. If triangle is equilateral,isosceles or scalene

  #include<stdio.h>
  int main()
  {
     float a,b,c;
     printf("enter sides:\n");
     scanf("%d %d %d",&a,&b,&c);
     if(a==b==c)
        printf("equi");
     else if(a==b || a==c || b==c)
        printf("iso");
    else
        printf("scalene");
      return 0;
  }

